<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <Authors>HeboTech</Authors>
    <Product>HeboTech ATLib</Product>
    <Version>0.0.1-alpha</Version>
    <PackageId>HeboTech.ATLib</PackageId>
    <PackageTags>atcommand at command gsm sms hayes</PackageTags>
    <Description>An extensible async AT command library.</Description>
    <RepositoryUrl>https://github.com/hbjorgo/ATLib</RepositoryUrl>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/hbjorgo/ATLib</PackageProjectUrl>
    <PackageReleaseNotes>https://github.com/hbjorgo/ATLib/releases</PackageReleaseNotes>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="Communication\ScriptCommunicator.cs" />
    <Compile Remove="Communication\SocketCommunicator.cs" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Cyotek.CircularBuffer" Version="1.0.3" />
    <PackageReference Include="System.IO.Ports" Version="4.7.0" />
  </ItemGroup>

</Project>
